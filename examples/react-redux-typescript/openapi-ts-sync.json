[
  {
    "name": "main",
    "path": "http://localhost:8081/openapi.json",
    "debug": true,
    "generators": [
      {
        "type": "./Generator/SchemaGenerator",
        "workingDir": "./src/api/main/schemas/",
        "template": "./node_modules/openapi-typescript-sync/templates/schemaInterface.handlebars"
      },
      {
        "type": "./Generator/ServiceGenerator",
        "workingDir": "./src/api/main/services/",
        "template": "./node_modules/openapi-typescript-sync/templates/serviceFunctions.handlebars",
        "options" : {
          "apiUrlConstant": {
            "constant": "SSO_API_URL",
            "import": "import {SSO_API_URL} from '../../settings';"
          }
        }
      }
    ]
  }
]
